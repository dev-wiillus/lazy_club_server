		"@nestjs/typeorm": "^8.1.4",
		"@types/express": "^4.17.13",
		"@types/jest": "27.5.0",
		"@types/joi": "^17.2.3",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/mailgun-js": "^0.22.12",
		"@types/node": "^18.0.0",
