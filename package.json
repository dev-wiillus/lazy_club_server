		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/graphql": "^10.0.16",
		"@nestjs/jwt": "^8.0.1",
		"@nestjs/mapped-types": "*",
		"@nestjs/passport": "^8.2.2",
		"@nestjs/platform-express": "^8.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/passport-local": "^1.0.34",
		"acorn": "8.7.1",
		"ajv": "6.9.1",
		"apollo-server-core": "^3.9.0",
		"apollo-server-express": "^3.9.0",
		"bcrypt": "^5.0.1",
		"browserslist": "4.21.1",
		"check-peer-dependencies": "^4.1.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"cross-env": "^7.0.3",
		"express": "4.18.1",
		"graphql": "^16.5.0",
		"joi": "^17.6.0",
		"jsonwebtoken": "^8.5.1",
		"mailgun-js": "^0.22.0",
